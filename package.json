{
  "name": "aubiter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "modulize-wasm": "node ./wasm/gravity/modulize.js",
    "wasm": "wasm-pack build ./wasm/gravity --target web && npm run modulize-wasm",
		"wasm:watch": "onchange \"./wasm/gravity/src/**/*\" -- npm run wasm",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "onchange": "^7.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0"
  },
  "type": "module",
  "dependencies": {
    "vite-plugin-wasm-pack": "^0.1.12"
  }
}
